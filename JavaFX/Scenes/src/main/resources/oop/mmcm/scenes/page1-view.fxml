<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefWidth="480" prefHeight="200" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.12" fx:controller="oop.mmcm.scenes.controllers.ScenesController">
    <center>
        <VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>

            <Label text="Page 1" />
        </VBox>
    </center>

    <bottom>
        <ButtonBar>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>

            <buttons>
                <Button text="Previous" onAction="#previous"/>
                <Button text="Next" onAction="#next"/>
            </buttons>
        </ButtonBar>
    </bottom>
</BorderPane>
