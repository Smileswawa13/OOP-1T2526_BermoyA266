<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.geometry.Insets?>

<GridPane alignment="TOP_LEFT" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml"
          fx:controller="oop.mmcm.layoutdemo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <ImageView fitHeight="200" fitWidth="200" preserveRatio="true" GridPane.columnIndex="0" GridPane.rowIndex="0">
        <Image url="@/images/dogie.gif"/>
    </ImageView>

    <ImageView fitHeight="200" fitWidth="200" preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="0">
        <Image url="@/images/chuck.jpeg"/>
    </ImageView>

    <ImageView fitHeight="200" fitWidth="200" preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="0">
        <Image url="@/images/pepe.webp"/>
    </ImageView>

    <ImageView fitHeight="200" fitWidth="200" preserveRatio="true" GridPane.columnIndex="0" GridPane.rowIndex="1">
        <Image url="@/images/ricky_roll.gif"/>
    </ImageView>

    <ImageView fitHeight="200" fitWidth="200" preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <Image url="@/images/u_mad.gif"/>
    </ImageView>
</GridPane>
